<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../css/admindashboard.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <title>Admin | Statistics</title>
</head>
<body>
    
    <%-include('./partials/admindesign.ejs') %>
    <section id="interface">
        <div class="navigation">
            <div class="n1">
                <div><i id="menu-btn" class="fa fa-bars"></i>
                </div>
                <div class="search">
                    <i class="fa fa-search"></i>
                    <input type="text" placeholder="Search">
                </div>
            </div>
            <div class="profile">
                <i class="fa fa-bell"></i>
                <i class="fa fa-user-circle"></i>
            </div>
        </div>
        <div class="statvalue">
            <div class="val-box">
                <i class="fa fa-users"></i>
                <div>
                    <h3>Users interactivity</h3>
                    <span>Monthly calculation</span>
                </div>
            </div>

            <div class="val-box">
                <i class="fa fa-shopping-cart"></i>
                <div>
                    <h3>Product Sales</h3>
                    <span>Weekly calculation</span>
                </div>
            </div>
            </div>
        <div class="stats">
            
            <div>
                <img src="../imgs/dashboardstatistics.png" alt="sales per month" width="500px">
            </div>
            <div>
                <img src="../imgs/users.webp" alt="sales per month" width="600px">
            </div>
        </div>
        <div class="stats">
         
    </section>
    
</body>
</html>